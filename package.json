{
  "name": "ember-test-assets",
  "version": "1.1.0",
  "description": "Add assets for tests easily and avoid 404 network errors.",
  "keywords": [
    "acceptance",
    "integration",
    "ember",
    "ember-addon",
    "helper",
    "assets",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/san650/ember-test-assets.git"
  },
  "license": "MIT",
  "author": "Santiago Ferreira",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
